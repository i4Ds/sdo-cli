  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Current File",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "justMyCode": false
    },
    {
      "name": "Python: SDO CLI get events",
      "type": "python",
      "request": "launch",
      "stopOnEntry": false,
      "justMyCode": false,
      "program": "${workspaceRoot}/src/sdo/cli.py",
      "console": "integratedTerminal",
      "args": [
        "events",
        "get",
        "--start=2011-01-01T00:00:00",
        "--end=2020-12-31T23:59:59",
        "--event-type=FL"
      ]
    },
    {
      "name": "Python: SDO CLI Train",
      "type": "python",
      "request": "launch",
      "stopOnEntry": false,
      "justMyCode": false,
      "program": "${workspaceRoot}/src/sdo/cli.py",
      "console": "integratedTerminal",
      "args": [
        "sood",
        "ce_vae",
        "train",
        "--config-file=./config/ce-vae/run-1.yaml"
      ]
    },
    {
      "name": "Python: SDO CLI Predict",
      "type": "python",
      "request": "launch",
      "stopOnEntry": false,
      "justMyCode": false,
      "program": "${workspaceRoot}/src/sdo/cli.py",
      "console": "integratedTerminal",
      "args": [
        "sood",
        "ce_vae",
        "predict",
        "--config-file=./config/ce-vae/run-1.yaml"
      ]
    },

    {
      "name": "Python: SDO CLI Generate",
      "type": "python",
      "request": "launch",
      "stopOnEntry": false,
      "justMyCode": false,
      "program": "${workspaceRoot}/src/sdo/cli.py",
      "console": "integratedTerminal",
      "args": [
        "sood",
        "ce_vae",
        "generate",
        "--config-file=./config/ce-vae/run-1.yaml"
      ]
    },
    {
      "name": "Python: SDO CLI GOES Download",
      "type": "python",
      "request": "launch",
      "stopOnEntry": false,
      "justMyCode": false,
      "program": "${workspaceRoot}/src/sdo/cli.py",
      "console": "integratedTerminal",
      "args": [
        "goes",
        "download",
        "--start=2010-01-01T00:00:00",
        "--end=2020-12-31T23:59:59",
        "--output=./tmp/new"
      ]
    },
    {
      "name": "Python: SDO CLI GOES Get",
      "type": "python",
      "request": "launch",
      "stopOnEntry": false,
      "justMyCode": false,
      "program": "${workspaceRoot}/src/sdo/cli.py",
      "console": "integratedTerminal",
      "args": [
        "goes",
        "get",
        "--timestamp=2015-06-01T02:20:00",
        "--cache-dir=./tmp"
      ]
    }
  ]
}
